{
  "manifest_version": 3,
  "name": "CopyText Extractor",
  "version": "1.0",
  "description": "Extracts the value of the copyText variable from a webpage.",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
